{% macro traverseStyledItem items firstCounter secondCounter colorArray imageArray %}
{% if secondCounter.count < items.count %}
  {% if items[secondCounter.count].name == "colors" %}
    {%call appendColor items items[secondCounter.count] firstCounter secondCounter colorArray imageArray%} {#游댮游댮#}
  {% elif items[secondCounter.count].name == "icons" %}
    {%call appendIcon items items[secondCounter.count] firstCounter secondCounter colorArray imageArray%} {#游리游리#}
  {% endif%}
{% endif %}
{% endmacro %} 
