  {% set firstCounter %}{{firstCounter}}0{% endset %}
  {% if firstCounter.count <= colorFolder.items.count %} 
  {% call appendColor  parentFolder colorFolder firstCounter secondCounter colorArray imageArray%} {#游댮游댮#}
  {% else %}
  {{colorArray}}
  {% set firstCounter %}{{""}}{% endset %}
  {% set secondCounter %}{{secondCounter}}0{% endset %}
    {% call traverseStyledItem parentFolder firstCounter secondCounter colorArray imageArray %} {#游릭游릭#}
  {% endif %}
  {% break %}
  {% endif %}
  {% if forloop.last %}
